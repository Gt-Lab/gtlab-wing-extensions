{
  "name": "wing-newts",
  "displayName": "New TypeScript Class",
  "description": "New TypeScript Class",
  "version": "1.0.0",
  "publisher": "gtlab",
  "engines": {
    "wing": "^3.0.6"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.svg",
  "repository": {
		"type": "git",
		"url": "https://github.com/gt-lab/wing-newts.git"
	},
  "license": "MIT",
  "activationEvents": [
    "onCommand:extension.showNewTsClassPopup"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.showNewTsClassPopup",
        "title": "New TypeScript Class"
      }
    ]
  },
  "scripts": {
    "wing:prepublish": "node ./node_modules/egretwing/bin/compile",
    "compile": "node ./node_modules/egretwing/bin/compile -watch -p ./src",
    "postinstall": "node ./node_modules/egretwing/bin/install"
  },
  "devDependencies": {
    "egretwing": "^3.0.4",
    "typescript": "^1.7.5"
  }
}
